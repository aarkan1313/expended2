  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "clean": "if exist dist rmdir /s /q dist && if exist node_modules\\.cache rmdir /s /q node_modules\\.cache",
    
    "_comment": "=== Git Automation Scripts ===",
    "git:save": "git add . && git commit -m \"save: work in progress\" && git push",
    "git:commit": "git add . && git commit -m",
    "git:deploy": "npm run format && git add . && git commit -m \"deploy: production ready\" && git push",
    "git:backup": "git add . && git commit -m \"backup: emergency save\" && git push",
    "git:quick": "git add . && git commit -m \"quick save\" && git push"
  },
